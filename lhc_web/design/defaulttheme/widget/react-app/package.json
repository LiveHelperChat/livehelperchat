{
  "name": "LHCWidget",
  "version": "1.0.0",
  "description": "Live Helper chat Widget",
  "main": "src/index.html",
  "scripts": {
    "dev": "parcel watch src/react.app.js --no-source-maps --out-dir ../../js/widgetv2/",
    "dev-ie": "parcel watch src/react.app.ie.js --no-source-maps --out-dir ../../js/widgetv2/",
    "build": "parcel build src/react.app.js --no-source-maps --detailed-report --out-dir ../../js/widgetv2/",
    "build-ie": "parcel build src/react.app.ie.js --no-source-maps --detailed-report --out-dir ../../js/widgetv2/",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "parcel --help"
  },
  "author": "Remigijus Kiminas",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "react-i18next": "^11.3.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap.native": "^2.0.27",
    "custom-event-polyfill": "^1.0.7",
    "howler": "^2.1.2",
    "html-react-parser": "^0.10.0",
    "i18next": "^19.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.12.0",
