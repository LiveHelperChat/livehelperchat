{
  "authors": [
    {
      "email": "remdex@gmail.com",
      "name": "Remigijus Kiminas",
      "homepage": "https://livehelperchat.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "vendor-dir": "lib/vendor",
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "description": "Live Helper Chat - live support for your website. Featuring web and desktop clients. Compatible Windows, Linux, Mac. Desktop client powered by QT. Supports mobile phones based on XMPP service providers (GTalk, Xabber, Mono etc.)",
  "homepage": "https://livehelperchat.com",
  "keywords": [
    "php",
    "live support",
    "livehelperchat",
    "qt",
    "xmpp"
  ],
  "license": [
    "GPL-3.0-or-later",
    "LGPL-3.0-or-later"
  ],
  "repositories": [
    {
      "type":"package",
      "package": {
        "name": "php-imap/php-imap",
        "version":"master",
        "source": {
          "url": "https://github.com/remdex/php-imap.git",
          "type": "git",
          "reference":"master"
        },
        "autoload": {
          "psr-4": {
            "PhpImap\\": "src/PhpImap"
          }
        }
      }
    },
    {
      "type":"package",
      "package": {
        "name": "tgallice/fb-messenger-sdk",
        "version":"master",
        "source": {
          "url": "https://github.com/LiveHelperChat/fb-messenger-sdk.git",
          "type": "git",
          "reference":"master"
        },
        "autoload": {
          "psr-4": {
            "Tgallice\\FBMessenger\\": "src"
          }
        }
      }
    },
    {
      "type":"package",
      "package": {
        "name": "longman/telegram-bot",
        "version":"master",
        "source": {
          "url": "https://github.com/LiveHelperChat/telegram-core.git",
          "type": "git",
          "reference":"develop"
        },
        "autoload": {
          "psr-4": {
            "Longman\\TelegramBot\\": "src"
          }
        }
      }
    }
  ],
  "minimum-stability": "stable",
  "name": "livehelperchat/livehelperchat",
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "aws/aws-sdk-php": "3.x",
    "colinmollenhour/credis": "~1.2",
    "daneren2005/php-resque": "dev-master#5facd365d2e9761d95dd9ee6c63b7fafcf11955d",
    "deeplcom/deepl-php": "^1.3",
    "elasticsearch/elasticsearch": "~7.0",
    "endroid/qr-code": "5.1.0",
    "giggsey/libphonenumber-for-php": "^8.12.44",
    "league/oauth2-server": "^8.3",
    "longman/telegram-bot": "dev-master#79e5e3abc82d130aa791bd767bd0980f7b85067c",
    "michelf/php-markdown": "^2.0",
    "minishlink/web-push": "^7.0",
    "nickdnk/graph-sdk": "^8.0.1",
    "php-imap/php-imap": "dev-master#6b9a4647777387ecba4f8e32dd7fd31d0a6456a0",
    "slim/slim": "^4.14.0",
    "sonata-project/google-authenticator": "^3.0@dev",
    "symfony/event-dispatcher": "^7.1",
    "tgallice/fb-messenger-sdk": "dev-master#a0d103f38473891432965c0f5b38b142b5dbfc3c",
    "webklex/php-imap": "^5.1.0"
  },
  "type": "project"
}
